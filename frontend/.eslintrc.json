{
    "extends": ["airbnb", "prettier"],

    "plugins": ["prettier"],
  
    "rules": {
      "no-unused-vars": 1,
      "no-underscore-dangle": "off",
      "prefer-destructuring": 1,
      "arrow-body-style": 1,
      "no-console": 0,
      "no-use-before-define": "warn",
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}